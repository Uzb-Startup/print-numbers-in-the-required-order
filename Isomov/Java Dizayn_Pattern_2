import java.util.Scanner;

public class Dizayn_Pattern_2 {
    public static void main(String[] args) {
        Scanner scanner=new Scanner(System.in);
        for(int i=1;i<=9;i++){
            for(int k=1;k<i;k++){
                System.out.print(k);
            }
            System.out.println();
        }
}}
